{
  "name": "odd1out-site",
  "version": "1.0.0",
  "description": "The home of Odd1Out on the web",
  "main": "index.html",
  "dependencies": {},
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:html": "cp src/index.html dist/",
    "build:css": "cp -r src/css/ dist/css",
    "build:svg": "svgo -f src/svg -o dist/svg",
    "build": "npm run build:html && npm run build:css && npm run build:svg",
    "deploy": "surge dist odd1out.band",
    "yolo": "npm run build && npm run deploy"
  },
